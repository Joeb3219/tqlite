{
    "name": "@joeb3219/tqlite",
    "version": "1.0.0",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "license": "MIT",
    "scripts": {
        "gen:parser": "node ./node_modules/tspeg/tsbuild/cli.js grammar/sql.tspeg src/parser-autogen/parser.ts --include-grammar-comment=false --regex-flags=i",
        "gen:parser2": "jisons src/parser-autogen/parser.ts > grammar/sql.peg",
        "build": "tsc",
        "watch": "nodemon ./src/index.ts",
        "lint": "prettier --write ."
    },
    "dependencies": {
        "commander": "^10.0.0",
        "lodash": "^4.17.21",
        "ts-jest": "^29.0.5",
        "typescript": "^4.9.4"
    },
    "devDependencies": {
        "@types/jest": "^29.2.5",
        "@types/lodash": "^4.14.191",
        "@types/node": "^18.11.3",
        "jest": "^29.3.1",
        "nodemon": "^2.0.20",
        "prettier": "^2.8.1",
        "prettier-plugin-organize-imports": "^3.2.1",
        "ts-node": "^10.9.1",
        "tspeg": "^3.2.1"
    }
}
